      PROCEDURE CreateMonitoredItem(myNodeId est NodeId)
      
      myMonitoredItem est Opc.Ua.Client.MonitoredItem()
      
      myMonitoredItem.NodeClass=NodeClass.Variable
      myMonitoredItem.DisplayName=txbNodeId
      myMonitoredItem.StartNodeId=myNodeId
