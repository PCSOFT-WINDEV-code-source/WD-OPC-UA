     name : createOpcUaSession
     internal_properties : CAAAAAgAAACu+/0Ijo5n6WtoAKNuWZPLuwayvyFtKwyiAMJAuKklolrcHTol7aGrp/AY3i9eTkvP+cFJEGO+/TrywfLCyyOB2gmZQOodXGiscHk4vJpXt1RtM1/MvlCgNqdhJ+8LC9mPcmNNhTfYks61sHG1aB7Gs/aetzpcbM0skb4dMOgE+U3hgSPRoKRbTE60xIM8mGYls1uSxt93lg==
     procedure_id : 2084330264199970259
     type_code : 14
     code : |1-
      // Résumé : Session OPCUA
      //// Traitement automatique des erreurs :   exécuter le bloc de code CAS ERREUR
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE createOpcUaSession(sUtilisateur est une chaine = "", sMotDePasse est une chaine = "")
      
      // Identité de connexion
      UserId est un UserIdentity dynamique
      
      myConfiguredEndpoint est ConfiguredEndpoint(Null,myEndpointDescription,myEndpointConfiguration)
      
      // Connexion authentifiée ?
      si sUtilisateur <> "" ALORS
      	UserId = new useridentity(sUtilisateur,sMotDePasse)
      SINON
      	UserId = null
      FIN
      
      // Création de la session
      mySession = Session.Create(myApplication.ApplicationConfiguration,
      							myConfiguredEndpoint,
      							faux,
      							vrai,
      							"MySession" ,
