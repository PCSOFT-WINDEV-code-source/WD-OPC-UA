           p_codes :
            -
              code : |1-
               
               
               
               bSessionOpen est un booléen	
               
               
               // On spécifie qu'on accepte les certificats qui ne sont pas "de confiance"
               // Permet de s'abstraire de la gestion des certificat ici
               myApplication.ApplicationConfiguration.SecurityConfiguration.AutoAcceptUntrustedCertificates = vrai
               
               myApplication.ApplicationConfiguration.CertificateValidator:add_CertificateValidation(DotNetDélégué("certificateValidation","CertificateValidationEventHandler"))
               
               // On doit se connecter ou se déconnecter ?
               si btCreateSession..Libellé = "Connexion" ALORS
               	// Récupère les objets de connexion est de configuration
               	si pas GetUAEndpoints() ALORS
               		Erreur("Impossible de se connecter.", "Vérifiez l'adresse du serveur.")
               		retour
               	FIN
               	
               	// On se connecte au serveur
               	si pas CreateOpcUaSession(sai_utilisateur,SAI_Mot_de_passe)	 ALORS
               		Erreur("Impossible de se connecter.", "Vérifiez l'adresse du serveur.")
               		RETOUR
               	FIN
               	
               	bSessionOpen = vrai
               	
               	GR_Connecté..grisé = faux
               	
               	ToastAffiche("Connecté")
               	
               	Disposition1[2]..Visible = vrai
               	Disposition1[3]..Visible = vrai
               	Disposition1[4]..Visible = vrai
               	Disposition1[5]..Visible = vrai
               SINON
               	// Déconnexion
               	closeOpcUaSession()
               	bSessionOpen = faux
               	GR_Connecté..Grisé = vrai
               	
               	ToastAffiche("Déconnecté")
               	
               	Disposition1[2]..Visible = faux
               	Disposition1[3]..Visible = Faux
               	Disposition1[4]..Visible = Faux
               	Disposition1[5]..Visible = Faux
               FIN
               	
