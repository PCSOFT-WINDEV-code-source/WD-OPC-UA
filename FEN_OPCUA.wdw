      PROCÉDURE GetBuiltInType(myNodeId est NodeId, szBuiltInType est chaine)
      
      request est RequestHeader Dynamic
      request = allouer RequestHeader
      
      maxAge est entier
      maxAge = 1000
      timeStampToReturn est TimestampsToReturn Dynamic
      timeStampToReturn= TimestampsToReturn.Both
      nodesToRead est ReadValueIdCollection dynamic
      nodesToRead = allouer ReadValueIdCollection
      myResult est DataValueCollection dynamic
      myResult = allouer DataValueCollection
      diagnosticsInfo est DiagnosticInfoCollection dynamic
      diagnosticsInfo = allouer DiagnosticInfoCollection
      nodetoRead est ReadValueId
      nodetoRead.NodeId=myNodeId
      nodetoRead.AttributeId=Attributes.DataType
      nodesToRead.Add(nodetoRead)
      
      // Lecture sur le serveur
      mySession.Read(request,maxAge,timeStampToReturn,nodesToRead,myResult,diagnosticsInfo)
      
      // Traitement du resultat
      resultSize est un entier
      resultSize=myResult.get_Count()
      si resultSize>0 alors
      	ii est entier
      	myValue est Opc.Ua.DataValue
      	pour ii=0 _à_ resultSize-1
      		myValue<-myResult.get_Item(ii)
      		si (myValue<>Null) alors
      			myint est entier=1
      			si myValue.WrappedValue<>Null alors
      				szBuiltInType = myValue.Value.ToString()
      			fin				
      		fin
      	fin
      fin
